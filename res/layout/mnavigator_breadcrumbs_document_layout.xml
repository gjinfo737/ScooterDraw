<?xml version="1.0" encoding="utf-8"?>
<copilot.module.breadcrumbs.layout.MBreadcrumbsDocumentLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:layout_gravity="center_horizontal"
	android:gravity="center_horizontal">
	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/mnavigator_document_label_linearLayout"
		android:orientation="vertical"
		android:layout_marginTop="20dp"
		android:layout_marginLeft="10dp"
		android:layout_gravity="center_horizontal"
		android:gravity="center_horizontal">
		<ImageView
			android:src="@drawable/theme_std_doctype_document128x128"
			android:layout_width="wrap_content"
			android:id="@+id/mBreadcrumbsDocumentLayoutIcon"
			android:layout_height="wrap_content">
		</ImageView>
		<TextView
			android:layout_width="wrap_content"
			android:id="@+id/mBreadcrumbsDocumentLayoutDocumentNameTextView"
			android:text="Document Name Goes Here"
			android:layout_height="wrap_content"
			android:textStyle="bold">
		</TextView>
	</LinearLayout>
</copilot.module.breadcrumbs.layout.MBreadcrumbsDocumentLayout>
